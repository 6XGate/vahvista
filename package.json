{
  "name": "vuev",
  "version": "1.0.0",
  "description": "Simple composable validation framework",
  "private": false,
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint src tests .eslintrc.js",
    "coverage": "nyc npm test",
    "test": "ava"
  },
  "keywords": [],
  "author": "Matthew Holder (https://www.sleepingcats.org/)",
  "license": "MIT",
  "engines": {
    "node": ">=12.9.0"
  },
  "ava": {
    "files": [
      "tests/**/*",
      "!tests/utils"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "esm"
    ],
    "nodeArguments": [
      "--enable-source-maps"
    ]
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "ava": "^3.15.0",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "esm": "^3.2.25",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  }
}
